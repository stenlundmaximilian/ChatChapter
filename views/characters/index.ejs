<table>
    <tr id="table-top">
        <th>Name</th>
        <th>Hobby</th>
        <th>Trait</th>
        <th>ID</th>
        <th></th>
        <th></th>
    </tr>
    <% characters.forEach((character) => { %>
        <tr>
          <td><%= character.name %></td>
          <td><%= character.hobby %></td>
          <td><%= character.trait %></td>
          <td><%= character._id %></td>
          <td>Edit</td> <!-- Add the appropriate link or button to edit the character -->
          <td>Delete</td> <!-- Add the appropriate link or button to delete the character -->
        </tr>
    <% }); %>
</table>